{
  "name": "agenda",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "nvm": ". $HOME/.nvm/nvm.sh && nvm install && nvm use",
    "prestart": "npm run nvm",
    "preinstall": "npm run nvm",
    "dev": "npm run nvm && vite",
    "build-splider": "sass ./src/assets/custom-splidejs.scss ./dist/custom-splidejs.css ",
    "build-tailwind": "tailwindcss -o ./dist/tailwind.css",
    "build": "vite build && npm run build-tailwind && npm run build-splider",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@internationalized/date": "^3.5.2",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@splidejs/svelte-splide": "^0.2.9",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^20.11.25",
    "autoprefixer": "^10.4.17",
    "bits-ui": "^0.19.5",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "path": "^0.12.7",
    "postcss": "^8.4.35",
    "rollup": "^4.14.0",
    "rollup-plugin-svelte": "^7.2.0",
    "sass": "^1.71.1",
    "svelte": "^4.2.11",
    "svelte-check": "^3.6.9",
    "svelte-loading-spinners": "^0.3.4",
    "svelte-preprocess": "^5.1.3",
    "svelte-radix": "^1.0.3",
