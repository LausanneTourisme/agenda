    "tailwind-variants": "^0.2.0",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3",
    "vite": "^5.1.4"
  },
  "dependencies": {
    "lucide-svelte": "^0.350.0",
    "moment": "^2.30.1",
    "svelte-cloudinary": "^1.3.1",
    "svelte-drawer-component": "^1.2.2",
    "svelte-i18n": "^4.0.0"
  }
